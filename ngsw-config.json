{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ],
        "urls": [
          "https://fonts.gstatic.com/s/roboto/v30/KFOmCnqEu92Fr1Mu4mxK.woff2",
          "/"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data-api",
      "urls": [
        "https://dattebayo-api.onrender.com/characters",
        "https://media.istockphoto.com/id/1674096889/es/foto/divertido-gatito-sacando-la-lengua-lamiendo-la-pantalla-aislado-sobre-fondo-rosa-pastel.jpg?s=2048x2048&w=is&k=20&c=OKXttobgjCcW9D7V1uXx11B_1dUIzoqfBSglSpXflW0="
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 5,
        "maxAge": "10m",
        "timeout": "20s"
      }
    }
  ]
}
